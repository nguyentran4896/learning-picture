<!DOCTYPE html>
<html lang="en" ng-app="LearningEnglish">

<head>
	<title>Ambition | <%= title %></title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- FONT CSS-->
	<link type="text/css" rel="stylesheet" href="//fonts.googleapis.com/css?family=Pacifico">
	<link type="text/css" rel="stylesheet" href="//fonts.googleapis.com/css?family=Cuprum:400,700">
	<link type="text/css" rel="stylesheet" href="/font/font-icon/font-awesome/css/font-awesome.css">
	<link type="text/css" rel="stylesheet" href="/font/font-icon/font-oganic/flaticon.css">
	
	<!-- LIBRARY CSS-->
	<link type="text/css" rel="stylesheet" href="/libs/bootstrap/css/bootstrap.min.css">
	<link type="text/css" rel="stylesheet" href="/libs/animate/animate.css">
	<link type="text/css" rel="stylesheet" href="/libs/slick-slider/slick.css">
	<link type="text/css" rel="stylesheet" href="/libs/slick-slider/slick-theme.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">
	<!-- STYLE CSS-->
	<link type="text/css" rel="stylesheet" href="/css/styles.css">
	<link type="text/css" rel="stylesheet" href="/css/upload.css">
	
	<script src="/libs/jquery/jquery-2.2.4.min.js"></script>
	
	<script src="/libs/angularjs/angular.min.js"></script>
	<script src="//www.gstatic.com/firebasejs/4.0.0/firebase.js"></script>
	<script src="//cdn.firebase.com/libs/angularfire/2.3.0/angularfire.min.js"></script>

	<script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="https://sdk.clarifai.com/js/clarifai-latest.js"></script>
	<script src="/lib/uploader/uploader.js"></script>

	<script src="/js/common.js"></script>
	<script type="text/javascript" src="/js/verser.js"></script>
	<script src="/libs/scrollbar/jquery.nicescroll.min.js" ></script>
</head>

<body ng-controller="shareController" class="pre-loader">
	<!--BEGIN PAGE LOADER-->
	<div class="body-wrapper" >
		<!-- WRAPPER CONTENT-->
		<div class="wrapper-content">
			<!-- HEADER-->
			<header>
				<div class="bg-transparent">
					<div class="header-topbar">
						<div class="hamburger-menu">
							    <span></span>
							    <span></span>
							    <span></span>
						</div>
                        <div class="container">
                        	<ul class="topbar-left list-unstyled list-inline">
                                <li>
                                	<img class="user-avatar" width="30" src="https://scontent-hkg3-1.xx.fbcdn.net/v/t1.0-9/18740419_1398808056874032_6423833359628514647_n.jpg?oh=89d30eede3cc2c6756a72ffe42d14f16&oe=5AE1ACB2" alt="">
                                	<a href="#" class="link">{{userName}}</a>
                                </li>
                            </ul>
                            <ul class="topbar-right list-unstyled list-inline style-2">
                                <li class="sub-menu"><a href="javascript:void(0)" class="link"> <span>Language Translate</span><i class="icons-2 fa fa-angle-double-down"></i></a>
                                    <ul class="dropdown-topbar">
						                          <li><a class="link" style="cursor: pointer;" ng-click='updateLanguage("vi", "Vietnamese")'> Vietnamese</a></li>
						                          <li><a class="link" style="cursor: pointer;" ng-click='updateLanguage("fr", "France")'> France</a></li>
						                          <li><a class="link" style="cursor: pointer;" ng-click='updateLanguage("ja", "Japanese")'> Japanese</a></li>
						                          <li><a class="link" style="cursor: pointer;" ng-click='updateLanguage("ko", "Korean")'> Korean</a></li>
						                          <li><a class="link" style="cursor: pointer;" ng-click='updateLanguage("th", "ThaiLand")'> ThaiLand</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
					<div class="header-main">
						<div class="container">
							<div class="header-main-wrapper">
								<div class="hamburger-menu-mobile"><i class="icons fa fa-bars"></i></div>
								<div class="logo-wrapper"><a href="/" class="logo">Ambition</a></div>
								<nav class="navigation">
									<ul class="nav-links nav navbar-nav">
                    <li>
                      <a href="/" class="main-menu">
                        <span class="text">Home</span>
                      </a>
                    </li>
                    <li>
                      <a href="/search" class="main-menu">
                        <span class="text">Search By Photo</span>
                      </a>
                    </li>
                    <li>
                      <a href="/mapping-word" class="main-menu">
                        <span class="text">Mapping Word</span>
                      </a>
                    </li>
                    <li class="active">
                      <a href="/verser" class="main-menu">
                        <span class="text">Study with friend</span>
                      </a>
                    </li>
									</ul>
								</nav>
							</div>
						</div>
					</div>
				</div>
			</header>
			<!-- MAIN CONTENT-->
			<div class="main-contents">
				<section class="wrapper-verser padding-top-160 padding-bottom-100">
					<div class="container">

						<div class="col-xs-12" ng-show="isChoosingImage">
							
							<input type="text" id="input" value="https://firebasestorage.googleapis.com/v0/b/elpicture-86fb4.appspot.com/o/images%2Fmountains.jpg?alt=media&token=e2359c6e-69da-4afd-bc35-4b5ca4e8ca39">

							<button id="button">go go go</button>

							<div id="drop-area" class="dm-uploader text-center" style="height: 300px">
								<h3>Drag &amp; drop Images here</h3>
								<div class="btn btn-primary ">
									<span>Open the file Browser</span>
									<input type="file" title="Click to add Files" id="file" name='file' onchange="angular.element(this).scope().uploadImage(this)"/>
								</div>
							</div>

						</div>

						<div class="wrapper-review-image text-center">
							<div class="review-image has-image">
								<img src="https://firebasestorage.googleapis.com/v0/b/elpicture-86fb4.appspot.com/o/images%2Fmountains.jpg?alt=media&token=e2359c6e-69da-4afd-bc35-4b5ca4e8ca39" alt="img">
							</div>
						</div>
					
						<div class="list-items list-col-5" ng-show="!isChoosingImage">
							<div class="item" ng-repeat="card in array">
								<div 
								ng-click="setSelected($index, card)"
								class="card" ng-class="{'active' : card.userSelect == userKey,
																						'': !card.userSelect,
																						'inactive': card.userSelect && card.userSelect != userKey && !card.isWrong,
																						'wrong': card.isWrong && card.userSelect
																					}">
									<div class="text">{{ card.name }}</div>
								</div>
							</div>
								
						</div>
					</div>
				</section>
			</div>
			
			<div class="wrapper-gamer">
				<div class="gamer-01">
					<div class="user-name">
						<img class="user-avatar" width="30px" style="border-radius:50%"
						src="https://scontent-hkg3-1.xx.fbcdn.net/v/t1.0-9/18740419_1398808056874032_6423833359628514647_n.jpg?oh=89d30eede3cc2c6756a72ffe42d14f16&oe=5AE1ACB2" alt="">
						<div class="name">Nguyen</div>
					</div>
					<div class="user-point">Point: {{ 13 + (array | filter:{'userSelect': userKey}).length }}</div>
				</div>

				<div class="gamer-02">
					<div class="user-name">
						<img class="user-avatar" width="30px" style="border-radius:50%"
						src="https://scontent-hkg3-1.xx.fbcdn.net/v/t31.0-1/c282.0.960.960/p960x960/10506738_10150004552801856_220367501106153455_o.jpg?oh=089d9dc6f4956b95e8d8e73fac1c7ea5&oe=5ADA1212" alt="">
						<div class="name">Nhan</div>
					</div>
					<div class="user-point">Point: {{ 18 + (array | filter:{'-userSelect': userKey}).length }}</div>
				</div>

				<!-- Count Down -->
				<div class="sc-count-down st-georgia text-c">
				    <div class="coming-soon count-down" data-expire="{{timer}}">
				    	<img src="images/more-image/alarm-clock.png" width="20" alt="">
				        <div class="main-count-wrapper hidden">
				            <div class="main-count">
				                <div class="time days flip">
				                    <span class="count curr top">00</span>
				                </div>
				                <div class="stat-label">days</div>
				            </div>
				        </div>
				        <div class="main-count-wrapper hidden">
				            <div class="main-count">
				                <div class="time hours flip">
				                    <span class="count curr top">00</span>
				                </div>
				                <div class="stat-label">hours</div>
				            </div>
				        </div>
				        <div class="main-count-wrapper">
				            <div class="main-count">
				                <div class="time minutes flip">
				                    <span class="count curr top">00</span>
				                </div>
				                <div class="stat-label">mins</div>
				            </div>
				        </div>
				        <div class="main-count-wrapper">
				            <div class="main-count">
				                <div class="time seconds flip">
				                    <span class="count curr top">00</span>
				                </div>
				                <div class="stat-label">secs</div>
				            </div>
				        </div>
				    </div>
				</div>
			</div>
		</div>

  <div id="live-chat">
      <header class="clearfix">
        Live Chat
        <a href="javascript:void(0)" class="chat-close">x</a>

        <span class="chat-message-counter">3</span>
      </header>
      <div class="chat">
        <div class="chat-history">

          <div class="chat-message clearfix" ng-repeat="chat in liveChats">
            <img src="http://lorempixum.com/32/32/people" alt="" width="32" height="32">
            <div class="chat-message-content clearfix">
              <span class="chat-time">{{chat.timeStamp | date:'HH:mm'}}</span>
              <h5>{{userName}}</h5>
              <p>{{ chat.message }}</p>
            </div> <!-- end chat-message-content -->
          </div> <!-- end chat-message -->

        </div> <!-- end chat-history -->
        <!-- <p class="chat-feedback">Your partner is typing…</p> -->
          <fieldset>
            <input type="text" placeholder="Type your message…" autofocus ng-model="nowMessage"
            ng-keyup="!$event.shiftKey && $event.keyCode == 13 && sendNonUserChat(nowMessage)">
            <input type="hidden">
          </fieldset>
      </div> <!-- end chat -->
    </div> <!-- end live-chat -->

	</div>
	<div class="wrapper-loader-dots">
		<div class="sk-chasing-dots">
			<div class="sk-child sk-dot1"></div>
			<div class="sk-child sk-dot2"></div>
		</div>
	</div>
	<!-- LIBRARY JS-->
	
	<script src="/libs/smooth-scroll/jquery-smoothscroll.min.js"></script>
	<script src="/libs/wow-js/wow.min.js"></script>
	<script src="/libs/slick-slider/slick.min.js"></script>
	<!-- MAIN JS-->
	<script src="/js/main.js"></script>
	
	<!-- LOADING SCRIPTS FOR PAGE-->
	<script src="/libs/parallax/jquery.parallax-scroll.min.js"></script>

	
</body>

</html>